Cell[BoxData[
 RowBox[{
  RowBox[{"youngProjector2", "[", 
   RowBox[{
    RowBox[{"youngDiagram", "[", "projectorPartition_", "]"}], ",", 
    RowBox[{"youngDiagram", "[", "representationPartition_", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"numberOfBoxes", "=", 
       RowBox[{"Total", "[", "projectorPartition", "]"}]}], ",", 
      RowBox[{"template", "=", 
       RowBox[{"diagramToOrderedTableau", "[", 
        RowBox[{"youngDiagram", "[", "projectorPartition", "]"}], "]"}]}], 
      ",", 
      RowBox[{"representationTableau", "=", 
       RowBox[{"youngTableau", "[", 
        RowBox[{"diagramToOrderedTableau", "[", 
         RowBox[{"youngDiagram", "[", "representationPartition", "]"}], "]"}],
         "]"}]}], ",", "rowIndices", ",", "columnIndices", ",", 
      "symmetrisers", ",", "antisymmetrisers", ",", "normalisation"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"Print", "[", "template", "]"}], ";"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"rowIndices", "=", 
      RowBox[{"reshapeRaggedArray", "[", 
       RowBox[{
        RowBox[{"Range", "[", "numberOfBoxes", "]"}], ",", "template"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", "rowIndices", "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"columnIndices", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"rowIndices", ",", 
        RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", "columnIndices", "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"symmetrisers", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"symmetriser", "[", 
         RowBox[{"#", ",", "representationTableau"}], "]"}], "&"}], "/@", 
       "rowIndices"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"antisymmetrisers", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"antisymmetriser", "[", 
         RowBox[{"#", ",", "representationTableau"}], "]"}], "&"}], "/@", 
       "columnIndices"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"symmetrisers", "=", 
      RowBox[{"Dot", "@@", "symmetrisers"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"antisymmetrisers", "=", 
      RowBox[{"Dot", "@@", "antisymmetrisers"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"normalisation", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", 
         RowBox[{"hookNumber", "[", 
          RowBox[{"youngTableau", "[", "template", "]"}], "]"}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"Times", "@@", 
         RowBox[{"Factorial", "/@", "projectorPartition"}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"Times", "@@", 
         RowBox[{"Factorial", "/@", 
          RowBox[{"Length", "/@", "columnIndices"}]}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"Length", "/@", "rowIndices"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"Length", "/@", "columnIndices"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", "normalisation", "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"normalisation", "*", 
      RowBox[{"symmetrisers", ".", "antisymmetrisers"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
