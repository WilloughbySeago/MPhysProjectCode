Cell[BoxData[
 RowBox[{
  RowBox[{"testRepresentationSample", "[", 
   RowBox[{
    RowBox[{"n_Integer", "/;", 
     RowBox[{"n", ">=", "1"}]}], ",", 
    RowBox[{"youngTableau", "[", "numbers_", "]"}], ",", "samples_Integer"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"yt", "=", 
       RowBox[{"youngTableau", "[", "numbers", "]"}]}], ",", 
      RowBox[{"permutations", "=", 
       RowBox[{"GroupElements", "[", 
        RowBox[{"SymmetricGroup", "[", "n", "]"}], "]"}]}], ",", 
      "multiplicationTable", ",", "i", ",", "j", ",", "success"}], "}"}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"samples", ">=", 
        RowBox[{"n", "!"}]}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"testRepresentation", "[", 
         RowBox[{"n", ",", "yt"}], "]"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"multiplicationTable", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"i", ",", "j"}], "}"}], "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", 
               RowBox[{"n", "!"}]}], "}"}], "]"}], ",", 
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", 
               RowBox[{"n", "!"}]}], "}"}], "]"}]}], "}"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"representation", "[", 
            RowBox[{
             RowBox[{"permutations", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "yt"}], "]"}], ".", 
           RowBox[{"representation", "[", 
            RowBox[{
             RowBox[{"permutations", "[", 
              RowBox[{"[", "j", "]"}], "]"}], ",", "yt"}], "]"}]}], "-", 
          RowBox[{"representation", "[", 
           RowBox[{
            RowBox[{"PermutationProduct", "[", 
             RowBox[{
              RowBox[{"permutations", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", 
              RowBox[{"permutations", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], "]"}], ",", "yt"}], 
           "]"}]}]}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "samples"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"success", "=", 
      RowBox[{
       RowBox[{"AllTrue", "[", 
        RowBox[{
         RowBox[{"#", "==", "0"}], "&"}], "]"}], "@", 
       RowBox[{"Flatten", "[", "multiplicationTable", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Assert", "[", "success", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{"success", ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Success: \>\"", ",", 
         RowBox[{"\"\<n = \>\"", "<>", 
          RowBox[{"ToString", "[", "n", "]"}], "<>", "\"\< \>\""}], ",", 
         "yt"}], "]"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Failure\>\"", ",", "multiplicationTable"}], "]"}]}], 
      "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
