Cell["\<\
The following function takes a tableau and permutation and applies the \
permutation to the tableau\
\>", "Text",
 CellChangeTimes->{{3.878549204449996*^9, 
  3.8785492218966465`*^9}},ExpressionUUID->"c4aa7ee7-db75-4696-88c6-\
7409f4678fc4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"applyPermutationToTableau", "[", 
   RowBox[{"permutation_Cycles", ",", 
    RowBox[{"youngTableau", "[", "numbers_", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"numberOfBoxes", "=", 
       RowBox[{"Length", "[", 
        RowBox[{"Flatten", "[", "numbers", "]"}], "]"}]}], ",", 
      "newTableau"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"newTableau", "=", 
      RowBox[{"Range", "[", "numberOfBoxes", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"newTableau", "=", 
      RowBox[{"Permute", "[", 
       RowBox[{"newTableau", ",", "permutation"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"newTableau", "=", 
      RowBox[{"reshapeRaggedArray", "[", 
       RowBox[{"newTableau", ",", "numbers"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"youngTableau", "[", "newTableau", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->"In[76]:=",ExpressionUUID->"78abf0e7-88f0-4122-9497-b1b59fd932dd"],

Cell[TextData[{
 "We can now calculate the representation of a given permutation, represented \
with Mathematica\[CloseCurlyQuote]s built in ",
 StyleBox["Cycles", "Code"],
 " type, in a given representation labelled by a Young tableau of a given \
shape"
}], "Text",
 CellChangeTimes->{{3.878025759439967*^9, 
  3.8780258153985834`*^9}},ExpressionUUID->"2e9580e4-5ba3-4bdb-a508-\
9f0f42bac067"],

Cell[BoxData[
 RowBox[{
  RowBox[{"representation", "[", 
   RowBox[{"permutation_Cycles", ",", 
    RowBox[{"youngTableau", "[", "numbers_", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"standardTableaux", "=", 
       RowBox[{"generateStandardTableaux", "[", 
        RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"numberOfBoxes", "=", 
       RowBox[{"Length", "[", 
        RowBox[{"Flatten", "[", "numbers", "]"}], "]"}]}], ",", 
      "\[IndentingNewLine]", "permutedTableaux", ",", "\[IndentingNewLine]", 
      "tableauxPermutations", ",", "\[IndentingNewLine]", "orderedTableau"}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"orderedTableau", "=", 
      RowBox[{"youngTableau", "[", 
       RowBox[{"reshapeRaggedArray", "[", 
        RowBox[{
         RowBox[{"Range", "[", "numberOfBoxes", "]"}], ",", "numbers"}], 
        "]"}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
       "tableau", " ", "filled", " ", "with", " ", "numbers", " ", "1"}], ",",
        " ", "2", ",", " ", "3", ",", " ", 
       RowBox[{"...", " ", "in", " ", "order"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"tableauxPermutations", "=", 
      RowBox[{"tableauToPermutation", "/@", "standardTableaux"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "turn", " ", "standard", " ", "tableaux", " ", "into", " ", 
       "permutations", " ", "acting", " ", "on", " ", "the", " ", "ordered", 
       " ", "tableaux"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"tableauxPermutations", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"PermutationProduct", "[", 
         RowBox[{"permutation", ",", "#"}], "]"}], "&"}], "/@", 
       "tableauxPermutations"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{"compute", " ", "product", " ", "of", " ", "permutations"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "apply", " ", "product", " ", "permutation", " ", "to", " ", "ordered", 
       " ", "tableaux"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"permutedTableaux", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"applyPermutationToTableau", "[", 
         RowBox[{"#", ",", "orderedTableau"}], "]"}], "&"}], "/@", 
       "tableauxPermutations"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"permutedTableaux", "=", 
      RowBox[{"decomposeTableau", "/@", "permutedTableaux"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "decompose", " ", "result", " ", "into", " ", "standard", " ", 
       "tableaux", " ", "using", " ", "Garnir"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"compute", " ", "representation", " ", "matrix"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Distribute", "[", 
          RowBox[{"standardTableauDotProduct", "[", 
           RowBox[{
            RowBox[{"standardTableaux", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ",", "#"}], "]"}], "]"}], "&"}], 
        "/@", "permutedTableaux"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "standardTableaux", "]"}]}], "}"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->"In[77]:=",ExpressionUUID->"0e970625-cb14-4d89-98fa-2b7d3735134e"],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Rho]", ":", 
         SubscriptBox[
          StyleBox["S", "TI"], 
          StyleBox["n", "TI"]], "\[Rule]", 
         StyleBox["GL", FontSlant -> "Plain"], 
         RowBox[{"(", 
           StyleBox["V", "TI"], ")"}]}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\rho \\colon S_n \\to \\mathrm{GL}(V)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "24704e03-901c-4f50-bd88-05fc88d710d8"],
 " is a representation of the symmetric group then for ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Sigma]", ",", "\[Tau]", "\[Element]", 
         SubscriptBox[
          StyleBox["S", "TI"], 
          StyleBox["n", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\sigma, \\tau \\in S_n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "682c4a53-f797-4d10-8f3b-7fa5f12090df"],
 " we should have ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Rho]", 
         RowBox[{"(", "\[Sigma]", ")"}], "\[Rho]", 
         RowBox[{"(", "\[Tau]", ")"}], "\[LongEqual]", "\[Rho]", 
         RowBox[{"(", "\[Sigma]\[Tau]", ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\rho(\\sigma) \\rho(\\tau) = \\rho(\\sigma \\tau)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3d1a5037-1e44-4831-a5b5-96a76155566a"],
 ", since a representation is by definition a homomorphism.\nWe can use this, \
in the form ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Rho]", 
         RowBox[{"(", "\[Sigma]", ")"}], "\[Rho]", 
         RowBox[{"(", "\[Tau]", ")"}], "-", "\[Rho]", 
         RowBox[{"(", "\[Sigma]\[Tau]", ")"}], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\rho(\\sigma) \\rho(\\tau) - \\rho(\\sigma\\tau)=0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "65381fee-3ac3-4bce-b8ed-83861f818399"],
 ", with subtraction just the usual subtraction of matrices, to check that \
the representations we are computing have the correct relationships. We do so \
here by constructing the multiplication tables for some of the smaller \
symmetric groups"
}], "Text",
 CellChangeTimes->{{3.8785492398516803`*^9, 3.878549241768445*^9}, {
  3.879153836148737*^9, 
  3.8791540271411314`*^9}},ExpressionUUID->"bd0cf2ed-61d7-46c4-ba6d-\
9164819b0b03"],

Cell[BoxData[
 RowBox[{
  RowBox[{"testRepresentation", "[", 
   RowBox[{
    RowBox[{"n_Integer", "/;", 
     RowBox[{"n", ">=", "1"}]}], ",", 
    RowBox[{"youngTableau", "[", "numbers_", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"yt", "=", 
       RowBox[{"youngTableau", "[", "numbers", "]"}]}], ",", 
      RowBox[{"permutations", "=", 
       RowBox[{"GroupElements", "[", 
        RowBox[{"SymmetricGroup", "[", "n", "]"}], "]"}]}], ",", 
      "multiplicationTable", ",", "success"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"multiplicationTable", "=", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"representation", "[", 
           RowBox[{
            RowBox[{"permutations", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ",", "yt"}], "]"}], ".", 
          RowBox[{"representation", "[", 
           RowBox[{
            RowBox[{"permutations", "[", 
             RowBox[{"[", "j", "]"}], "]"}], ",", "yt"}], "]"}]}], "-", 
         RowBox[{"representation", "[", 
          RowBox[{
           RowBox[{"PermutationProduct", "[", 
            RowBox[{
             RowBox[{"permutations", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"permutations", "[", 
              RowBox[{"[", "j", "]"}], "]"}]}], "]"}], ",", "yt"}], "]"}]}], 
        "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"n", "!"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", 
          RowBox[{"n", "!"}]}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"success", "=", 
      RowBox[{
       RowBox[{"AllTrue", "[", 
        RowBox[{
         RowBox[{"#", "==", "0"}], "&"}], "]"}], "@", 
       RowBox[{"Flatten", "[", "multiplicationTable", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Assert", "[", "success", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{"success", ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Success: \>\"", ",", 
         RowBox[{"\"\<n = \>\"", "<>", 
          RowBox[{"ToString", "[", "n", "]"}], "<>", "\"\< \>\""}], ",", 
         "yt"}], "]"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Failure\>\"", ",", "multiplicationTable"}], "]"}]}], 
      "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.87915403573071*^9, 3.879154211834762*^9}, {
   3.879154316933441*^9, 3.8791543983774137`*^9}, 3.8791544327867665`*^9, {
