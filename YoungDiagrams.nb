   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"unpermutedList", "=", 
       RowBox[{"Flatten", "[", "numbers", "]"}]}], ",", "permutedList", ",", 
      "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"permutedList", "=", "unpermutedList"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Iterate", " ", "through", " ", "flattened", " ", "lists", " ", 
       "making", " ", "necessary", " ", "swaps"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "swaps", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"permutedList", "[", 
         RowBox[{"[", 
          RowBox[{"swaps", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "1"}], "]"}], "]"}], "]"}], "]"}], "=", 
        RowBox[{"unpermutedList", "[", 
