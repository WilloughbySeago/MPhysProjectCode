Cell[BoxData[
 RowBox[{
  RowBox[{"applyGarnir", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"misordering", "=", 
       RowBox[{"findMisordering", "[", 
        RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ",", 
      "position", ",", 
      RowBox[{"canonicalTableau", "=", 
       RowBox[{"reshapeRaggedArray", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"Length", "[", 
           RowBox[{"Flatten", "[", "numbers", "]"}], "]"}], "]"}], ",", 
         "numbers"}], "]"}]}], ",", "rowLength", ",", "row1", ",", "row2", 
      ",", "swaps"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"isStandardTableau", "[", 
        RowBox[{"sortRows", "[", 
         RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}], "]"}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"sortRows", "[", 
         RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}], "]"}]}], "]"}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"position", "=", 
      RowBox[{"Position", "[", 
       RowBox[{
        RowBox[{"reshapeRaggedArray", "[", 
         RowBox[{
          RowBox[{"Range", "[", 
           RowBox[{"Max", "[", 
            RowBox[{"Flatten", "[", "numbers", "]"}], "]"}], "]"}], ",", 
          "numbers"}], "]"}], ",", 
        RowBox[{"misordering", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "position", " ", "in", " ", "ragged", " ", "array", " ", "of", " ", 
       "the", " ", "misordering"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"rowLength", "=", 
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"canonicalTableau", "[", 
         RowBox[{"[", 
          RowBox[{"position", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}], "]"}], "]"}], "]"}], "-", 
       RowBox[{"position", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}], "+", "1"}]}], " ", ";", 
     RowBox[{"(*", " ", 
      RowBox[{
      "number", " ", "of", " ", "boxes", " ", "in", " ", "row", " ", "after", 
       " ", 
       RowBox[{"(", 
        RowBox[{"and", " ", "including"}], ")"}], " ", "misordered", " ", 
       "box"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"row1", "=", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{"misordering", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{
         RowBox[{"misordering", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "+", "rowLength", "-", "1"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"row2", "=", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"misordering", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "+", "rowLength"}], ",", 
        RowBox[{"misordering", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"swaps", "=", 
      RowBox[{"Rest", "@", 
       RowBox[{"generateSwaps", "[", 
        RowBox[{"row1", ",", "row2"}], "]"}]}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"Throw", " ", "away", " ", "first", " ", "result"}], ",", " ", 
       RowBox[{"its", " ", "the", " ", "identity", " ", "and", " ", "we", " ", 
        RowBox[{"don", "'"}], "t", " ", "want", " ", "it"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"applySwaps", "[", 
        RowBox[{
         RowBox[{"youngTableau", "[", "numbers", "]"}], ",", "#"}], "]"}], 
       "&"}], "/@", "swaps"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
