Cell[BoxData[
 RowBox[{
  RowBox[{"generateStandardTableaux", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"standardTableaux", "=", 
       RowBox[{"{", "}"}]}], ",", "numberOfBoxes", ",", "permutations", ",", 
      "i", ",", "yt", ",", "numberOfStandardTableau"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"numberOfBoxes", "=", 
      RowBox[{"Length", "[", 
       RowBox[{"Flatten", "[", "numbers", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"permutations", "=", 
      RowBox[{"Permutations", "[", 
       RowBox[{"Range", "[", "numberOfBoxes", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numberOfStandardTableau", "=", 
      RowBox[{"hookFormula", "[", 
       RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", 
        RowBox[{"numberOfBoxes", "!"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"yt", "=", 
         RowBox[{"sortRows", "@", 
          RowBox[{"youngTableau", "[", 
           RowBox[{"reshapeRaggedArray", "[", 
            RowBox[{
             RowBox[{"permutations", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "numbers"}], "]"}], 
           "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Not", "@", 
            RowBox[{"MemberQ", "[", 
             RowBox[{"standardTableaux", ",", "yt"}], "]"}]}], "&&", 
           RowBox[{"isStandardTableau", "[", "yt", "]"}]}], ",", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"standardTableaux", ",", "yt"}], "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "standardTableaux", "]"}], "==", 
           "numberOfStandardTableau"}], ",", 
          RowBox[{"Break", "[", "]"}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "standardTableaux"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.877247253484536*^9, 3.8772472878691783`*^9}, {
  3.877247360228814*^9, 3.8772475809811363`*^9}, {3.877247657701673*^9, 
  3.8772477487119913`*^9}, {3.8772477850085444`*^9, 3.8772478913870234`*^9}, {
