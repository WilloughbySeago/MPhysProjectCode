  RowBox[{"youngProjector", "[", 
   RowBox[{
    RowBox[{"youngTableau", "[", "projectorNumbers_", "]"}], ",", 
    RowBox[{"youngTableau", "[", "representationNumbers_", "]"}]}], "]"}], ":=", 
