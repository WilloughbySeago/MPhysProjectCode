Cell[CellGroupData[{

Cell["Shift", "Chapter",
 CellChangeTimes->{{3.8840637179974585`*^9, 
  3.884063722386284*^9}},ExpressionUUID->"fcca36fb-3940-48c6-977b-\
c43072a0f2d0"],

Cell["\<\
Often we want to combine multiple operations into one in a way that involves \
combining operations on, for example, two legs, with an operation on four \
legs. If we want to apply the 2-leg operator to the last two legs of the \
4-leg operator we need to shift it down, apply it, and then shift back up, \
the shifting can be done with the following function\
\>", "Text",
 CellChangeTimes->{{3.8840637355966454`*^9, 3.8840637770730605`*^9}, {
  3.884064156570051*^9, 
  3.8840641882295313`*^9}},ExpressionUUID->"99ee7fae-f5fc-40c4-a7b7-\
dc0e0d4b0907"],

Cell[BoxData[
 RowBox[{
  RowBox[{"shift", "[", 
   RowBox[{"currentLegs_List", ",", "shiftLegs_List", ",", 
    RowBox[{"youngTableau", "[", "representationNumbers_", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"swapPairs", ",", "permutation"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Assert", "[", 
      RowBox[{
       RowBox[{"Length", "[", "currentLegs", "]"}], "==", 
       RowBox[{"Length", "[", "shiftLegs", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"swapPairs", "=", 
      RowBox[{"Transpose", "[", 
       RowBox[{"{", 
        RowBox[{"currentLegs", ",", "shiftLegs"}], "}"}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Generate", " ", "list", " ", "of", " ", "legs", " ", "to", " ", 
       "swap"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"swapPairs", "=", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{"swapPairs", ",", 
        RowBox[{"{", 
         RowBox[{"x_", ",", "x_"}], "}"}]}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
       "Shifting", " ", "something", " ", "on", " ", "to", " ", "itself", " ",
         "causes", " ", "problems", " ", "as", " ", 
        RowBox[{"Mathematica", "'"}], "s", " ", "Cycle", " ", 
        RowBox[{"doesn", "'"}], "t", " ", "like", " ", 
        RowBox[{"Cycle", "[", 
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1"}], "}"}], "}"}], "]"}]}], ",", " ", 
       RowBox[{"so", " ", "remove", " ", "these"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"permutation", "=", 
      RowBox[{"PermutationProduct", "@@", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"Cycles", "[", 
           RowBox[{"{", "#", "}"}], "]"}], "&"}], "/@", "swapPairs"}], 
        ")"}]}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
       "Calculate", " ", "product", " ", "of", " ", "permutations", " ", 
        "directly"}], ",", " ", 
       RowBox[{
       "probably", " ", "faster", " ", "than", " ", "doing", " ", 
        "representations", " ", 
        RowBox[{"individually", "?"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"representation", "[", 
      RowBox[{"permutation", ",", 
       RowBox[{"youngTableau", "[", "representationNumbers", "]"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.8840642065035152`*^9, 3.8840642136076465`*^9}, {
  3.884705569462885*^9, 3.88470562348362*^9}, {3.8847568962793856`*^9, 
  3.8847568967244716`*^9}, {3.8847569499461684`*^9, 3.8847570259137344`*^9}},
 CellLabel->
