Cell[BoxData[
 RowBox[{
  RowBox[{"decomposeTableau", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"tableau", "=", 
       RowBox[{"sortRows", "[", 
        RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ",", 
      "tableauList"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Not", "@", 
        RowBox[{"isStandardTableau", "[", "tableau", "]"}]}], ",", 
       RowBox[{"tableauList", "=", 
        RowBox[{"applyGarnir", "[", "tableau", "]"}]}], ",", 
       RowBox[{"Return", "[", "tableau", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"-", 
      RowBox[{"Plus", "@@", 
       RowBox[{"decomposeTableau", "/@", "tableauList"}]}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
