Cell["\<\
We now write a function which takes a Young tableau and applies the Garnir \
relation to it once.\
\>", "Text",
 CellChangeTimes->{{3.8755381356045556`*^9, 
  3.8755381726445093`*^9}},ExpressionUUID->"3bcd39d9-ee1f-4da5-8cf5-\
d315ccd492b4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"applyGarnir", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"misordering", "=", 
       RowBox[{"findMisordering", "[", 
        RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ",", 
      "position", ",", 
      RowBox[{"canonicalTableau", "=", 
       RowBox[{"reshapeRaggedArray", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"Length", "[", 
           RowBox[{"Flatten", "[", "numbers", "]"}], "]"}], "]"}], ",", 
         "numbers"}], "]"}]}], ",", "rowLength", ",", "row1", ",", "row2", 
      ",", "swaps"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"isStandardTableau", "[", 
        RowBox[{"sortRows", "[", 
         RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}], "]"}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"sortRows", "[", 
         RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}], "]"}]}], "]"}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"position", "=", 
      RowBox[{"Position", "[", 
       RowBox[{
        RowBox[{"reshapeRaggedArray", "[", 
         RowBox[{
          RowBox[{"Range", "[", 
           RowBox[{"Max", "[", 
            RowBox[{"Flatten", "[", "numbers", "]"}], "]"}], "]"}], ",", 
          "numbers"}], "]"}], ",", 
        RowBox[{"misordering", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "position", " ", "in", " ", "ragged", " ", "array", " ", "of", " ", 
       "the", " ", "misordering"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"rowLength", "=", 
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"canonicalTableau", "[", 
         RowBox[{"[", 
          RowBox[{"position", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}], "]"}], "]"}], "]"}], "-", 
       RowBox[{"position", "[", 
        RowBox[{"[", 
