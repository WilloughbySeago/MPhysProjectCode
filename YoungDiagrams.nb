Cell["\<\
We start with a helper function to test if a list is sorted in decreasing \
order\
\>", "Text",
 CellChangeTimes->{{3.8732137111728363`*^9, 3.8732137211850967`*^9}, {
  3.8737877194699974`*^9, 
  3.87378772254761*^9}},ExpressionUUID->"da79993e-603c-4736-b991-\
e03c78895306"],

Cell[BoxData[
 RowBox[{
  RowBox[{"isSortedDescending", "[", 
   RowBox[{"{", "x_", "}"}], "]"}], ":=", "True", "  ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"base", " ", "case"}], ",", " ", 
    RowBox[{
    "a", " ", "one", " ", "element", " ", "list", " ", "is", " ", 
     "sorted"}]}], " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.8732132901055408`*^9, 3.8732134467388315`*^9}, {
  3.873213525634576*^9, 3.873213530069276*^9}, {3.873213655068102*^9, 
  3.873213686238578*^9}, {3.873214211918844*^9, 3.8732142185655174`*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"4ce6ba38-0681-4244-9b10-611f620663e6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"isSortedDescending", "[", "list_List", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"list", "[", 
     RowBox[{"[", "1", "]"}], "]"}], ">=", 
    RowBox[{"list", "[", 
     RowBox[{"[", "2", "]"}], "]"}]}], "&&", 
   RowBox[{"isSortedDescending", "[", 
    RowBox[{"Rest", "[", "list", "]"}], "]"}], "  ", 
   RowBox[{"(*", " ", 
