Cell[BoxData[
 RowBox[{
  RowBox[{"hookLengths", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"hookNumbers", "=", "numbers"}], ",", "i"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Iterate", " ", "over", " ", "tableau", " ", "and", " ", "compute", " ", 
      "hook", " ", "length", " ", "at", " ", "each", " ", "step"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "numbers", "]"}]}], ",", 
       RowBox[{"++", "i"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"j", "=", "1"}], ",", 
         RowBox[{"j", "<=", 
          RowBox[{"Length", "[", 
           RowBox[{"numbers", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}]}], ",", 
         RowBox[{"++", "j"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"hookNumbers", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
          RowBox[{
           RowBox[{"Length", "[", 
            RowBox[{"numbers", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "]"}], "-", "j", "+", 
           RowBox[{"Count", "[", 
            RowBox[{
             RowBox[{"numbers", "[", 
              RowBox[{"[", 
               RowBox[{"i", ";;"}], "]"}], "]"}], ",", 
             RowBox[{"x_", "/;", 
              RowBox[{
               RowBox[{"Length", "[", "x", "]"}], ">=", "j"}]}]}], 
            "]"}]}]}]}], " ", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"length", " ", "-", " ", 
           RowBox[{
           "j", " ", "gives", " ", "number", " ", "of", " ", "boxes", " ", 
            "left", " ", "in", " ", "row"}]}], ",", " ", 
          RowBox[{
           RowBox[{"count", "[", "...", "]"}], " ", "gives", " ", "number", 
           " ", "of", " ", "rows", " ", "from", " ", "current", " ", "row", 
           " ", "down", " ", "with", " ", "boxes", " ", "left", " ", "in", 
           " ", "the", " ", "current", " ", "column"}]}], " ", "*)"}], 
        "\[IndentingNewLine]", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"youngTableau", "[", "hookNumbers", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.877075219372003*^9, 3.8770752882220945`*^9}, {
   3.877247405864809*^9, 3.877247406454582*^9}, 3.8791533401909246`*^9},
