Cell[TextData[{
 "Next comes the recursive step. We set the head to the first time of the \
unsorted list, and the tail to the rest. We then recursively call the \
function twice on the tail, splitting at ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["k", "TI"], "-", "1"}], TraditionalForm], "errors" -> {}, 
     "input" -> "k-1", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ad9c94e9-9aa1-43cc-9103-8d8e5981a000"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["k", "TI"], TraditionalForm], "errors" -> {}, "input" -> "k", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "04c81711-fc02-4a6d-9806-8d9869d829c5"],
 " depending on which list we put the head into."
}], "Text",
 CellChangeTimes->{{3.8755364170936127`*^9, 
  3.87553647568874*^9}},ExpressionUUID->"29ce8b96-e903-44e0-9bfc-\
cda70a1624c1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"generatePermutedLists", "[", 
   RowBox[{"list_List", ",", "k_Integer", ",", "left_", ",", "right_"}], 
   "]"}], ":=", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"head", ",", "tail"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"head", "=", 
       RowBox[{"First", "[", "list", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"tail", "=", 
       RowBox[{"Rest", "[", "list", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"generatePermutedLists", "[", 
         RowBox[{"tail", ",", 
          RowBox[{"k", "-", "1"}], ",", 
          RowBox[{"Append", "[", 
           RowBox[{"left", ",", "head"}], "]"}], ",", "right"}], "]"}], ",", 
        RowBox[{"generatePermutedLists", "[", 
         RowBox[{"tail", ",", "k", ",", "left", ",", 
          RowBox[{"Append", "[", 
           RowBox[{"right", ",", "head"}], "]"}]}], "]"}]}], "}"}]}]}], 
    "\[IndentingNewLine]", "]"}], "/;", 
   RowBox[{"k", "<=", 
    RowBox[{"Length", "[", "list", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.8755364763910117`*^9, 3.875536598082314*^9}},
 CellLabel->"In[53]:=",ExpressionUUID->"9cf6644a-ef0a-4a71-9341-a0cd38e3bd5d"],

Cell[TextData[{
 "Next we define a helper function which will take in one of the pairs \
produced by this function and then generate the permutation we need to apply \
to the Young Tableau. This permutation is represented as a list of lists, \
with each sublist containing two elements, ",
 StyleBox["{a, b}", "Code"],
 ", meaning \[OpenCurlyDoubleQuote]take the thing in spot ",
 StyleBox["a", "Code"],
 " and replace it with the thing in spot ",
 StyleBox["b", "Code"],
 "\[CloseCurlyDoubleQuote] we\[CloseCurlyQuote]ll call this a ",
 StyleBox["swap",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.875536662437046*^9, 
  3.8755367750892663`*^9}},ExpressionUUID->"56e58c48-648a-498f-b36c-\
7efd8cd99d02"],

Cell[BoxData[
 RowBox[{
  RowBox[{"pairToSwaps", "[", "pairs_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"maxNumber", ",", "flattenedListPair", ",", "permutedList"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"flattenedListPair", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"pairs", "/.", 
        RowBox[{"pair", "->", "List"}]}], "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "flatten", " ", "input", " ", "and", " ", "convert", " ", "pairs", " ", 
       "to", " ", "lists"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"maxNumber", "=", 
      RowBox[{"Max", "[", "flattenedListPair", "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "need", " ", "to", " ", "generate", " ", "a", " ", "list", " ", "of", 
       " ", "at", " ", "least", " ", "as", " ", "many", " ", "numbers", " ", 
       "as", " ", "the", " ", "maximum", " ", "box", " ", "number"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"permutedList", "=", 
      RowBox[{"Join", "[", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"1", ",", 
          RowBox[{
           RowBox[{"Min", "[", "flattenedListPair", "]"}], "-", "1"}]}], 
         "]"}], ",", "flattenedListPair"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"DeleteCases", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Range", "[", "maxNumber", "]"}], ",", "permutedList"}], 
         "}"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"a_", ",", "a_"}], "}"}]}], "]"}]}]}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "create", " ", "pairs", " ", "from", " ", "lists", " ", "and", " ", 
     "remove", " ", "elements", " ", "that", " ", 
     RowBox[{"don", "'"}], "t", " ", "change"}], " ", "*)"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
