a2b8a02a1f60"],

Cell[BoxData[
 RowBox[{
  RowBox[{"findMisordering", "[", 
   RowBox[{"youngTableau", "[", "numbers_", "]"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"tableau", "=", 
       RowBox[{"sortRows", "[", 
        RowBox[{"youngTableau", "[", "numbers", "]"}], "]"}]}], ",", 
      RowBox[{"(*", " ", 
       RowBox[{"sort", " ", "rows", " ", "of", " ", "array"}], " ", "*)"}], 
      "\[IndentingNewLine]", "rowIndex", ",", "comparison", ",", 
      RowBox[{"position", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", " ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"rowIndex", "=", "1"}], ",", 
       RowBox[{"rowIndex", "<=", 
        RowBox[{
         RowBox[{"Length", "[", "numbers", "]"}], "-", "1"}]}], ",", 
       RowBox[{"rowIndex", "++"}], ",", "  ", 
       RowBox[{"(*", " ", 
        RowBox[{
        "for", " ", "each", " ", "row", " ", "in", " ", "the", " ", 
         "tableau"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"comparison", "=", 
         RowBox[{"compareRaggedLists", "[", 
          RowBox[{
           RowBox[{"numbers", "[", 
            RowBox[{"[", "rowIndex", "]"}], "]"}], ",", 
           RowBox[{"numbers", "[", 
            RowBox[{"[", 
             RowBox[{"rowIndex", "+", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
        RowBox[{"(*", " ", 
         RowBox[{"Compare", " ", "rows", " ", "elementwise"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"position", "=", 
         RowBox[{"Position", "[", 
          RowBox[{"comparison", ",", "False", ",", "1"}], "]"}]}], ";", "  ", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Get", " ", "the", " ", "position", " ", "of", " ", "the", " ", 
          "first", " ", "occurence", " ", "of", " ", "False", " ", "in", " ", 
          "the", " ", "array"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"position", "!=", 
           RowBox[{"{", "}"}]}], ",", 
          RowBox[{"Break", "[", "]"}]}], "]"}]}]}], "]"}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "If", " ", "False", " ", "was", " ", "found", " ", "then", " ", "exit", 
       " ", "the", " ", "loop"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"position", "==", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", 
          RowBox[{"-", "1"}]}], "}"}], "]"}]}], "]"}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"In", " ", "standard", " ", "form"}], ",", " ", 
       RowBox[{"use", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", " ", 
          RowBox[{"-", "1"}]}], "}"}], " ", "as", " ", "a", " ", "result", 
        " ", "that", " ", "cannot", " ", "be", " ", "achieved", " ", "if", 
        " ", "not", " ", "sorted"}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"position", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}], "+", 
         RowBox[{"Length", "[", 
          RowBox[{"Flatten", "[", 
           RowBox[{"numbers", "[", 
            RowBox[{"[", 
             RowBox[{"1", ";;", 
              RowBox[{"rowIndex", "-", "1"}]}], "]"}], "]"}], "]"}], "]"}]}], 
        ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"position", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}], "+", 
         RowBox[{"Length", "[", 
          RowBox[{"Flatten", "[", 
           RowBox[{"numbers", "[", 
            RowBox[{"[", 
             RowBox[{"1", ";;", "rowIndex"}], "]"}], "]"}], "]"}], "]"}]}]}], 
       "}"}], "]"}]}]}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Not", " ", "in", " ", "standard", " ", "form"}], ",", " ", 
     RowBox[{
     "return", " ", "box", " ", "number", " ", "of", " ", "first", " ", 
      "unsorted", " ", "box"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
